<system xmlns="http://tail-f.com/ns/example/quagga/1.0">
  <hostname>edgerouter-north</hostname>
  <password>zebra</password>
  <router/>
  <interface>
    <name>eth0</name>
    <description>Default interface</description>
    <ip>
      <address>
	<address>10.2.2.1</address>
	<prefix-length>16</prefix-length>
      </address>
      <rip>
	<split-horizon>split</split-horizon>
      </rip>
      <ospf>
	<mtu-ignore>false</mtu-ignore>
	<network>broadcast</network>
      </ospf>
    </ip>
    <ipv6>
      <nd>
	<managed-config-flag>false</managed-config-flag>
	<other-config-flag>false</other-config-flag>
	<suppress-ra>true</suppress-ra>
      </nd>
    </ipv6>
    <bandwidth>100</bandwidth>
    <link-detect>false</link-detect>
    <multicast>if-default</multicast>
    <up>true</up>
  </interface>
  <interface>
    <name>eth1</name>
    <ip>
      <address>
	<address>10.7.0.1</address>
	<prefix-length>16</prefix-length>
      </address>
      <rip>
	<split-horizon>split</split-horizon>
      </rip>
      <ospf>
	<mtu-ignore>false</mtu-ignore>
	<network>broadcast</network>
      </ospf>
    </ip>
    <ipv6>
      <nd>
	<managed-config-flag>false</managed-config-flag>
	<other-config-flag>false</other-config-flag>
	<suppress-ra>true</suppress-ra>
      </nd>
    </ipv6>
    <link-detect>false</link-detect>
    <multicast>if-default</multicast>
    <up>true</up>
  </interface>
  <key-chain>
    <name>primary</name>
    <key>
      <id>40</id>
      <key-string>abba</key-string>
    </key>
  </key-chain>
  <ip>
    <forwarding>false</forwarding>
  </ip>
  <ipv6>
    <forwarding>false</forwarding>
  </ipv6>
  <log/>
  <stats>
    <ip/>
    <memory/>
  </stats>
  <table>0</table>
  <vpn>
    <ipsec>
      <defaults>
	<encryption-algo>des</encryption-algo>
	<hash-algo>md5</hash-algo>
      </defaults>
    </ipsec>
  </vpn>
</system>

