    <firewall>
        <family>
            <inet>
                <filter>
                    <name>re-protect</name>
                    <term>
                        <name>except-tcp-rate</name>
                        <from>
                            <source-prefix-list>
                                <name>ssh-clients</name>
                            </source-prefix-list>
                            <protocol>tcp</protocol>
                            <tcp-flags> (syn&amp;!ack)|fin|rst </tcp-flags>
                        </from>
                        <then>
                            <count>except-tcp-syn-rate</count>
                            <accept/>
                        </then>
                    </term>
                    <term>
                        <name>tcp-syn-rate</name>
                        <from>
                            <source-prefix-list>
                                <name>bgp-auto</name>
                            </source-prefix-list>
                            <protocol>tcp</protocol>
                            <tcp-flags> (syn&amp;!ack)|fin|rst </tcp-flags>
                        </from>
                        <then>
                            <policer>tcp-syn-policer</policer>
                            <count>tcp-syn-rate</count>
                            <accept/>
                        </then>
                    </term>
                    <term>
                        <name>icmp-rate</name>
                        <from>
                            <protocol>icmp</protocol>
                            <icmp-type>echo-request</icmp-type>
                            <icmp-type>echo-reply</icmp-type>
                            <icmp-type>unreachable</icmp-type>
                            <icmp-type>time-exceeded</icmp-type>
                            <icmp-type>timestamp</icmp-type>
                            <icmp-type>timestamp-reply</icmp-type>
                        </from>
                        <then>
                            <policer>icmp-policer</policer>
                            <count>icmp-rate</count>
                            <accept/>
                        </then>
                    </term>
                    <term>
                        <name>tacacs</name>
                        <from>
                            <source-prefix-list>
                                <name>tacacs-servers</name>
                            </source-prefix-list>
                            <protocol>tcp</protocol>
                            <port>tacacs</port>
                        </from>
                        <then>
                            <count>tacacs</count>
                            <accept/>
                        </then>
                    </term>
                    <term>
                        <name>ignore-tcp-chatter</name>
                        <from>
                            <protocol>tcp</protocol>
                            <port>135-139</port>
                            <port>1433</port>
                            <port>1434</port>
                            <port>445</port>
                            <port>80</port>
                        </from>
                        <then>
                            <count>chatter-tcp</count>
                            <discard>
                            </discard>
                        </then>
                    </term>
                    <term>
                        <name>ssh-authorized</name>
                        <from>
                            <source-prefix-list>
                                <name>ssh-clients</name>
                            </source-prefix-list>
                            <protocol>tcp</protocol>
                            <port>ssh</port>
                        </from>
                        <then>
                            <count>ssh-auth</count>
                            <accept/>
                        </then>
                    </term>
                    <term>
                        <name>netconf</name>
                        <from>
                            <source-prefix-list>
                                <name>ssh-clients</name>
                            </source-prefix-list>
                            <protocol>tcp</protocol>
                            <destination-port>830</destination-port>
                        </from>
                        <then>
                            <count>netconf</count>
                            <accept/>
                        </then>
                    </term>
                </filter>
            </inet>
        </family>
        <policer>
            <name>tcp-syn-policer</name>
            <if-exceeding>
                <bandwidth-limit>500k</bandwidth-limit>
                <burst-size-limit>15k</burst-size-limit>
            </if-exceeding>
            <then>
                <discard/>
            </then>
        </policer>
        <policer>
            <name>icmp-policer</name>
            <if-exceeding>
                <bandwidth-limit>5m</bandwidth-limit>
                <burst-size-limit>100k</burst-size-limit>
            </if-exceeding>
            <then>
                <discard/>
            </then>
        </policer>
        <policer>
            <name>ssh-policer</name>
            <if-exceeding>
                <bandwidth-limit>2m</bandwidth-limit>
                <burst-size-limit>50k</burst-size-limit>
            </if-exceeding>
            <then>
                <discard/>
            </then>
        </policer>
        <policer>
            <name>traceroute-policer</name>
            <if-exceeding>
                <bandwidth-limit>2m</bandwidth-limit>
                <burst-size-limit>50k</burst-size-limit>
            </if-exceeding>
            <then>
                <discard/>
            </then>
        </policer>
    </firewall>
